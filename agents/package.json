{
  "name": "matrix-agents",
  "version": "0.1.0",
  "description": "TypeScript coordination layer for Matrix flash loan arbitrage bot",
  "private": true,
  "type": "module",
  "workspaces": [
    "merovingian",
    "keymaker",
    "link",
    "shared"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "dev:merovingian": "npm run dev --workspace=merovingian",
    "dev:keymaker": "npm run dev --workspace=keymaker",
    "dev:link": "npm run dev --workspace=link"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "ethers": "^6.9.0",
    "viem": "^1.19.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
