{
  "name": "@matrix/dashboard",
  "version": "1.0.0",
  "description": "Matrix Command Center - Competitor Intelligence & Strategy Dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx scripts/startup.ts",
    "dev": "vite",
    "dev:api": "tsx watch src/api/server.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "tsc && vite build",
    "build:api": "tsc -p tsconfig.api.json",
    "start:api": "node dist/api/server.js",
    "preview": "vite preview",
    "lint": "eslint src/ --ext .ts,.tsx",
    "test": "vitest",
    "db:setup": "node db/setup.js",
    "db:migrate": "node db/migrate.js",
    "db:seed": "node db/seed.js",
    "db:seed-sample": "tsx scripts/seed-sample-data.ts",
    "ingest:prices": "tsx src/services/priceIngestion.ts",
    "ingest:competitors": "tsx src/services/competitorIngestion.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.8.0",
    "@tanstack/react-table": "^8.10.0",
    "axios": "^1.6.0",
    "better-sqlite3": "^11.10.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "d3": "^7.8.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.0",
    "viem": "^2.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.0",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
